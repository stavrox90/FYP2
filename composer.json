{
    "name": "zyev/collaborative-course-callendar",
    "description": "Collaborative Online Course Schedulings and Plannings.",
    "keywords": ["university", "collaborative", "course", "calendar", "scheduling", "planning"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "inrafilus",
            "email": "inrafilus@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0",
        "illuminate/view": "^9.0",
        "laminas/laminas-text": "^2.9",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.14"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["reka"]
}
